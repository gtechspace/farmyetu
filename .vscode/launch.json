{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome to localhost:8080",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Attach to Chrome",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Open contacts.html",
      "url": "file:///C:/Users/ruthj/farmyetu/contacts.html"
    },
    {
      "name": "Run npm start",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm start"
    }
  ]
}
